{
  "name": "keybinding-decorator",
  "main": "lib/index.js",
  "author": "abouthiroppy <git@about-hiroppy.com http://about-hiroppy.com",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Decorator for keybinding",
  "keywords": [
    "decorator",
    "keybinding",
    "mousetrap",
    "keymap"
  ],
  "files": ["lib"],
  "scripts": {
    "lint": "eslint .",
    "test": "nyc ava",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "watch": "npm run build -- -w",
    "build": "babel src -d lib",
    "prepublish": "npm run build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "postcover": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "tap": true,
    "failFast": true,
    "concurrency": 5
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "conventional-changelog-cli": "^1.3.2",
    "eslint": "^4.4.1",
    "eslint-config-sky": "^1.6.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.53.0",
    "flow-typed": "^2.1.5",
    "nyc": "^11.1.0"
  },
  "dependencies": {
    "mousetrap": "^1.6.1"
  }
}
